if evo chain / species / id == current creature
    take first evo

else
    do until ./evolves_to / species / id != current creature id

        get next evolves_to

get first spcies/url from chain node

if first species/url == current creature id {
    do exit
} else {
    repeat {

        get evolves to ARRAY

    } while ((current species/url != current creature id) && (evolves_to != nil))

    if (evolves_to != nil) {

        for each item in ARRAY
            add descendant

    }
}


